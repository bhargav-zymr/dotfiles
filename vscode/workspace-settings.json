{
  "folders": [
    {
      "path": "./"
    }
  ],
  "settings": {
    "typescript.referencesCodeLens.enabled": true,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.autoImportSuggestions.enabled": true,
    "files.exclude": {
      ".angulardoc.json": true,
      ".babelrc": true,
      ".editorconfig": true,
      ".eslintignore": true,
      ".eslintrc": false,
      ".gitattributes": true,
      ".gitignore": true,
      ".lesshintrc": true,
      ".prettierrc": false,
      ".vscode": true,
      "*.code-workspace": true,
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.svn": true,
      "**/*.css.map": {
        "when": "$(basename)"
      },
      "**/*.d.ts": true,
      "**/*.js": {
        "when": "$(basename).ts"
      },
      "**/*.js.map": {
        "when": "$(basename)"
      },
      "**/*.spec.ts": {
        "when": "$(basename).ts"
      },
      "**/CVS": true,
      "**/polyfills.ts": true,
      "**/test.ts": true,
      "e2e/": true,
      "karma.conf.js": true,
      "node_modules/": true,
      "package-lock.json": false,
      "package.json": false,
      "protractor.conf.js": true,
      "semantic.json": true,
      "tslint.json": false,
      "webpack.config.js": true
    },
    "search.exclude": {
      ".vscode": true,
      "/src/assets/fonts": true,
      "/src/assets/icon": true,
      "dist": true,
      "e2e/": true,
      "node_modules/": true,
      "node_modules/**": true
    },
    "typescript.tsdk": "node_modules/typescript/lib"
  }
}
